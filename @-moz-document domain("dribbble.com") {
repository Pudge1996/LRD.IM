    background-image: linear-gradient(270deg, #1b1b1b 50%, rgba(27,27,27,0.00) 100%);
    color: var(--white-text);
}
.scrolling-subnav .scroll.scroll-backward a{
    background-image: linear-gradient(270deg, rgba(27,27,27,0.00),#1b1b1b 50% 100%);
    color: var(--white-text);
}

input[type='text'], input[type='search'], input[type='password'], input[type='email'], input[type='url'], input[type='tel'], .selectize-input, textarea, .ql-container .ql-editor, select, .input-with-prefix{
    background-color: var(--tertiary-bg);
    color: var(--white-text);
}

